{
   "version": "0.9.1",
   "background_page": "background.html",
   "browser_action": {
      "default_icon": "img/icon_24.png",
      "default_title": "New Webnote"
   },
   "content_scripts": [ {
      "css": [ "css/webnotes.css" ],
      "js": [ "js/webnotes.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
   } ],
   "description": "Attach sticky notes to any webpage. Notes are automatically synchronized to all your Chrome installations, Handhelds and PCs",
   "icons": {
        "96": "img/icon_96.png",
        "128": "img/icon_128.png",
        "19": "img/icon_24.png",
        "24": "img/icon_24.png",
        "32": "img/icon_32.png",
        "48": "img/icon_48.png"
   },
   "name": "Syncpad Webnotes (beta)",
   "permissions": [ "tabs", "http://*/*", "https://*/*" ]   
}
